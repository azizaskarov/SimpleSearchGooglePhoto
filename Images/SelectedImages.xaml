<Window x:Class="SimpleSearchGooglePhoto.Images.SelectedImages"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleSearchGooglePhoto.Images"
        mc:Ignorable="d"
        Title="SelectedImages" Height="400" Width="900" WindowStartupLocation="CenterScreen">
    <Grid>
        <ItemsControl x:Name="ImageSelectControl"  >
            <ItemsControl.ItemsPanel  >
                <ItemsPanelTemplate >
                    <WrapPanel  Margin="0,0,0,0"/>
                </ItemsPanelTemplate>
            </ItemsControl.ItemsPanel>
        </ItemsControl>
    </Grid>
</Window>
